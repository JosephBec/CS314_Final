{
  "extends": [
    "react-app",
    "react-app/jest"
  ],
  "rules": {
    "no-unused-vars": ["warn", { 
      "varsIgnorePattern": "^_",
      "argsIgnorePattern": "^_",
      "ignoreRestSiblings": true
    }]
  },
  "overrides": [
    {
      "files": [
        "**/*.test.js",
        "**/*.spec.js",
        "**/cypress/**/*.js"
      ],
      "rules": {
        "no-unused-vars": "off"
      }
    }
  ]
}
